    var message = '', prefix = '[' + ( title || 'Reported Error' ) + '] ';

    try {
        message = ( prefix + ( err.message || '' ) + ' ' + ( err.stack || '' ) ).slice( 0, 500 );
    } catch( e ) {}

    if ( message ) {
        window.onerror( message );
    }

};
